        alert('ห้ามใช้บนหน้านี้ครับ!');
        return false;
    }
	else if(event.keyCode==93)
	{
		alert('ห้ามใช้บนหน้านี้ครับ!');
		return false;
	}
    else if(event.ctrlKey && event.shiftKey && event.keyCode==73)
    {
        alert('ห้ามใช้บนหน้านี้ครับ!')
