<script>
var nowfontsize = localStorage.getItem('fontsize');
$('.content').css('font-size',nowfontsize+'px')
if(nowfontsize != null || nowfontsize != 'NaN'){
	$('.content').css('font-size',nowfontsize)
}
var chagefont = function(data){
	var type = $(data).attr('data-type');
	var nowfontsize = localStorage.getItem('fontsize');
	if(nowfontsize == null || nowfontsize == 'NaN'){

		localStorage.setItem('fontsize',20);
		 nowfontsize = localStorage.getItem('fontsize');
	}
	switch (type){
		case '0':
				$('.content').css('font-size',nowfontsize*1-1)
			//localStorage.fontsize = nowfontsize*1-2;
			localStorage.setItem('fontsize',nowfontsize*1-1);
			break;
		case '1':
			$('.content').css('font-size',nowfontsize*1+1)
			localStorage.setItem('fontsize',nowfontsize*1+1);
			break;
		case '2':
			$('.content').css('font-size',20)
			localStorage.setItem('fontsize',20);
			break;
		default:
			$('.content').css('font-size',nowfontsize*1+1)
			localStorage.setItem('fontsize',nowfontsize*1+1);
	}

}
    $(window).on('keydown',function(event)
